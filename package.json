{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "start": "ng serve --configuration=local --o",
    "start:dev": "ng serve --configuration=development --o --host 0.0.0.0",
    "start:prod": "ng serve --configuration=production --o --host 0.0.0.0",
    "build:local": "ng build --configuration=local",
    "build:dev": "ng build --prod --configuration=development",
    "build:prod": "ng build --prod --configuration=production",
    "ng": "ng",
    "build": "ng build --prod",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "^8.2.13",
    "@angular/forms": "~8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@angular/upgrade": "~8.2.14",
    "@auth0/angular-jwt": "^3.0.1",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-in-memory-web-api": "~0.7.0",
    "core-js": "^2.6.1",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "^0.2.4",
    "node-sass": "^4.13.1",
    "rxjs": "^6.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngrx/schematics": "^7.0.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "codelyzer": "^5.0.1",
    "jasmine-marbles": "^0.4.1",
    "jest": "24.3.1",
    "jest-preset-angular": "7.0.0",
    "lodash": "^4.17.11",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "~5.4.1",
    "rxjs-compat": "^6.3.3",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "3.5.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coveragePathIgnorePatterns": [
      "/jest-config/",
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./jest-config/setup.ts"
    ],
    "moduleNameMapper": {
      "@appEnvironments/(.*)": "<rootDir>/src/environments/$1",
      "@appContainers/(.*)": "<rootDir>/src/app/containers/$1",
      "@appComponents/(.*)": "<rootDir>/src/app/components/$1",
      "@appServices/(.*)": "<rootDir>/src/app/services/$1",
      "@appStore/(.*)": "<rootDir>/src/app/store/$1",
      "@appGuards/(.*)": "<rootDir>/src/app/guards/$1",
      "@appModels/(.*)": "<rootDir>/src/app/models/$1",
      "@appAuthentication/(.*)": "<rootDir>/src/app/authentication/$1",
      "@appHttp/(.*)": "<rootDir>/src/app/http/$1"
    }
  },
  "repository": {}
}
